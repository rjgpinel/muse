
<mujoco>
    <include file="base_env.xml"></include>
    <asset>
        <texture builtin="flat" type="2d" name="box_tex" height="32" width="32" />
        <material name="box_mat" specular="0.5" shininess="0.2" reflectance="0" texture="box_tex" />
        <texture builtin="flat" type="2d" name="box_int_tex" height="32" width="32" />
        <material name="box_int_mat" specular="0.5" shininess="0.2" reflectance="0" texture="box_int_tex" />
        <texture builtin="flat" type="2d" name="lid_tex" height="32" width="32" />
        <material name="lid_mat" specular="0.5" shininess="0.2" reflectance="0" texture="lid_tex" />
        <texture builtin="flat" type="2d" name="lid_handle_tex" height="32" width="32" />
        <material name="lid_handle_mat" specular="0.5" shininess="0.2" reflectance="0" texture="lid_handle_tex" />
        <texture builtin="flat" type="2d" name="cube0_tex" height="32" width="32" />
        <material name="cube0_mat" specular="0.5" shininess="0.2" reflectance="0" texture="cube0_tex" />
        <texture builtin="flat" type="2d" name="cube1_tex" height="32" width="32" />
        <material name="cube1_mat" specular="0.5" shininess="0.2" reflectance="0" texture="cube1_tex" />
        <texture builtin="flat" type="2d" name="button0_box_tex" height="32" width="32" />
        <material name="button0_box_mat" specular="0.5" shininess="0.2" reflectance="0" texture="button0_box_tex" />
        <texture builtin="flat" type="2d" name="button0_tex" height="32" width="32" />
        <material name="button0_mat" specular="0.5" shininess="0.2" reflectance="0" texture="button0_tex" />
        <texture builtin="flat" type="2d" name="button1_box_tex" height="32" width="32" />
        <material name="button1_box_mat" specular="0.5" shininess="0.2" reflectance="0" texture="button1_box_tex" />
        <texture builtin="flat" type="2d" name="button1_tex" height="32" width="32" />
        <material name="button1_mat" specular="0.5" shininess="0.2" reflectance="0" texture="button1_tex" />
        <texture builtin="flat" type="2d" name="button2_box_tex" height="32" width="32" />
        <material name="button2_box_mat" specular="0.5" shininess="0.2" reflectance="0" texture="button2_box_tex" />
        <texture builtin="flat" type="2d" name="button2_tex" height="32" width="32" />
        <material name="button2_mat" specular="0.5" shininess="0.2" reflectance="0" texture="button2_tex" />
        <texture builtin="flat" type="2d" name="button3_box_tex" height="32" width="32" />
    </asset>

    <worldbody>
        <body mocap="true" name="box" pos="-0.4 0.0 0.002">
            <geom name="box_bottom" size="0.10 0.10 0.001" type="box"  material="box_mat" />
            <geom name="box_int_bottom" size="0.10 0.10 0.001" type="box"  material="box_int_mat" pos="0 0 0.001" />
            <geom name="box_side0" pos="0.0 0.10 0.04" size="0.10 0.04 0.001" euler="1.57079632679 0 0" type="box"  material="box_mat" />
            <geom name="box_int_side0" pos="0.0 0.10 0.04" size="0.10 0.04 0.001" euler="1.57079632679 0 0" type="box"  material="box_int_mat" />
            <geom name="box_side1" pos="0.0 -0.10 0.04" size="0.10 0.04 0.001" euler="1.57079632679 0 0" type="box"  material="box_mat" />
            <geom name="box_int_side1" pos="0.0 -0.10 0.04" size="0.10 0.04 0.001" euler="1.57079632679 0 0" type="box"  material="box_int_mat" />
            <geom name="box_side2" pos="0.10 0.0 0.04" size="0.04 0.10 0.001" euler="0 1.57079632679 0" type="box"  material="box_mat" />
            <geom name="box_int_side2" pos="0.10 0.0 0.04" size="0.04 0.10 0.001" euler="0 1.57079632679 0" type="box"  material="box_int_mat" />
            <geom name="box_side3" pos="-0.10 0.0 0.04" size="0.04 0.10 0.001" euler="0 1.57079632679 0" type="box"  material="box_mat" />
            <geom name="box_int_side3" pos="-0.10 0.0 0.04" size="0.04 0.10 0.001" euler="0 1.57079632679 0" type="box"  material="box_int_mat" />
        </body>
        <body name="lid" pos ="-0.4 0.0 0.08">
            <joint name="lid_joint" />
            <geom name="lid_top" pos="0.0 0.0 0.0" size="0.115 0.115 0.002" type="box"  material="lid_mat" />
            <body name="lid_handle" pos="0.0 0.0 0.017">
                <geom name="lid_handle" size="0.035 0.017 0.017" type="box" material="lid_handle_mat" />
            </body>
        </body>

        <body childclass="cube" name="cube0" pos="-0.4 0.0 0.025">
            <joint name="cube0_joint" />
            <geom name="cube0" size="0.025 0.025 0.025" type="box"  material="cube0_mat" />
            <site name="cube0" size="0.025 0.025 0.025" rgba="1 0 0 0" type="sphere" />
        </body>

        <body mocap="true" name="button0_box" pos="-0.6 0.1 0.01">
            <geom name="button0_box" size="0.025 0.025 0.01" type="box"  material="button0_box_mat" rgba="0.886 0.886 0.886 1" />
            <body name="button0" pos="0.0 0.0 0.02">
                <joint name="button0_joint" pos="0 0 0" axis="0 0 1" type="slide" group="3" damping="0" limited="true" range="-0.01 0" />
                <geom name="button0" size="0.015 0.01 0.01" type="cylinder"  material="button0_mat"/>
                <site name="button0_site" size="0.015 0.01 0.01" type="cylinder" rgba="0 0 0 0" />
            </body>
        </body>
        <body mocap="true" name="button1_box" pos="-0.6 0.0 0.01">
          <geom name="button1_box" size="0.025 0.025 0.01" type="box"  material="button1_box_mat" rgba="0.886 0.886 0.886 1" />
          <body name="button1" pos="0.0 0.0 0.02">
              <joint name="button1_joint" pos="0 0 0" axis="0 0 1" type="slide" group="3" damping="0" limited="true" range="-0.01 0" />
              <geom name="button1" size="0.015 0.01 0.01" type="cylinder"  material="button1_mat" />
              <site name="button1_site" size="0.015 0.01 0.01" type="cylinder" rgba="0 0 0 0" />
          </body>
      </body>
      <body mocap="true" name="button2_box" pos="-0.6 -0.1 0.01">
          <geom name="button2_box" size="0.025 0.025 0.01" type="box"  material="button2_box_mat" rgba="0.886 0.886 0.886 1" />
          <body name="button2" pos="0.0 0.0 0.02">
              <joint name="button2_joint" pos="0 0 0" axis="0 0 1" type="slide" group="3" damping="0" limited="true" range="-0.01 0" />
              <geom name="button2" size="0.015 0.01 0.01" type="cylinder"  material="button2_mat" />
              <site name="button2_site" size="0.015 0.01 0.01" type="cylinder" rgba="0 0 0 0" />
          </body>
      </body>

    </worldbody>

    <contact>
        <exclude body1="button0_box" body2="button0" />
        <exclude body1="button1_box" body2="button1" />
        <exclude body1="button2_box" body2="button2" />
    </contact>
    <sensor>
        <touch name="button0_sensor" site="button0_site" />
        <touch name="button1_sensor" site="button1_site" />
        <touch name="button2_sensor" site="button2_site" />
    </sensor>

</mujoco>
